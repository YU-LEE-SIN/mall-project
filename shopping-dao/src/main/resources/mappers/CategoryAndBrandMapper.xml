        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nf.dao.CategoryAndBrandDao">
    <insert id="insertCateAndBran" parameterType="com.nf.entity.CategoryAndBrand">
        insert into tb_category_brand(category_id,brand_id) values
        (#{categoryId},#{brandId})
